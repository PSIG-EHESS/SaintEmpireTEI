            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">qualifie la forme comme simple, composée, etc.
Suggested values include: 1] simple; 2] lemma; 3] variant; 4] compound; 5] derivative; 6] inflected; 7] phrase</a:documentation>
               <choice>
                  <value>simple</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">item lexical simple</a:documentation>
                  <value>lemma</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">l'entrée proprement dite</a:documentation>
                  <value>variant</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">variante</a:documentation>
                  <value>compound</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">mot formé d'items lexicaux simples</a:documentation>
                  <value>derivative</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">mot dérivé de l'entrée</a:documentation>
                  <value>inflected</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">mot dans une autre forme que la forme habituelle du dictionnaire</a:documentation>
                  <value>phrase</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">item lexical à plusieurs mots</a:documentation>
                  <data type="token">
                     <param name="pattern">[^\p{C}\p{Z}]+</param>
                  </data>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
